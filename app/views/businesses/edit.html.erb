<%= simple_form_for @business do |f| %>
	<%= f.select :category_id, @categories, html => {multipart => true } do |f| %>
	
	<%= f.input :name %>
	<%= f.input :logo %>
	<%= f.input :description %>
	<%= f.input :university %>
	<%= f.input :location %>
	<%= f.input :video %>
	<%= f.button :submit %>


<% end %>
