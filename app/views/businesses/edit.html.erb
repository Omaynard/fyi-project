<%= simple_form_for @business, :html => { :multipart => true } do |f| %>
	<%= f.select :category_id, @categories %>
	
	<%= f.input :name %>
	
	<%= f.input :description %>
	<%= f.input :university %>
	<%= f.input :location %>
	<%= f.input :video %>

    <%= f.file_field :logo %>
	<%= f.button :submit %>


<% end %>
