<%= simple_form_for @business do |f| %>
	<%= select_tag(:category_id, options_for_select(@categories), :prompt => "Select a catergory") %>
	<%= f.input :name %>
	<%= f.input :logo %>
	<%= f.input :description %>
	<%= f.input :university %>
	<%= f.input :location %>
	<%= f.input :video %>
	<%= f.button :submit %>


<% end %>